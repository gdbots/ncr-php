{
  "name": "gdbots/ncr",
  "description": "Node Content Repository for php.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "gdbots/pbjx": "3.x-dev",
    "gdbots/query-parser": "2.x-dev",
    "gdbots/schemas": "2.x-dev",
    "psr/cache": "^1",
    "psr/log": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "aws/aws-sdk-php": "^3.138",
    "ruflin/elastica": "^7.0.0-beta.4",
    "symfony/cache": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Ncr\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Ncr\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
