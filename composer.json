{
  "name": "gdbots/ncr",
  "description": "Node Content Repository for php.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/pbjx": "dev-es as 1.0.0",
    "gdbots/schemas": "dev-es as 1.0.0",
    "psr/log": "~1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php": "~3.19",
    "ruflin/elastica": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Ncr\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Ncr\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
