{
  "name": "gdbots/ncr",
  "description": "Node Content Repository for php.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/pbjx": "^2.1.1",
    "psr/cache": "^1",
    "psr/log": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "aws/aws-sdk-php": "^3.45",
    "gdbots/acme-schemas": "^1.1",
    "ruflin/elastica": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Ncr\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Ncr\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
