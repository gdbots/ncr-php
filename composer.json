{
  "name": "gdbots/ncr",
  "description": "Node Content Repository for php.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.1",
    "gdbots/pbjx": "^4.1",
    "psr/cache": "^1|^2|^3",
    "psr/log": "^1|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "aws/aws-sdk-php": "^3.138",
    "gdbots/acme-schemas": "^3.0",
    "ruflin/elastica": "^7.1",
    "symfony/cache": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Ncr\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Ncr\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
